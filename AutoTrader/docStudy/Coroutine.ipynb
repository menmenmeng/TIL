   "source": [
    "def coroutine2(x):\n",
    "    print(\">> Coroutine2 started : {}\".format(x))\n",
    "    y = yield x\n",
    "    print(\">> Coroutine2 Y received : {}\".format(y))\n",
    "    z = yield x + y\n",
    "    print(\">> Coroutine2 Z received : {}\".format(z))\n",
    "    yield z + x + y"
   ]
  },
